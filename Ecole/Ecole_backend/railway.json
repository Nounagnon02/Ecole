{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "composer install --optimize-autoloader --no-dev && php artisan optimize",
    "skipNpmInstall": true,
    "zeroConfig": false,
    "disableNode": true
  
  },
  "deploy": {
    "startCommand": "php artisan serve --host=0.0.0.0 --port=${PORT}"
  }
}